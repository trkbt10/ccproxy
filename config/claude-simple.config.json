{
  "defaults": {
    "providerId": "claude",
    "model": "claude-3-5-sonnet-20241022"
  },
  "providers": {
    "claude": {
      "type": "claude",
      "apiKey": "${ANTHROPIC_API_KEY:?Missing ANTHROPIC_API_KEY in environment}",
      "baseURL": "${ANTHROPIC_BASE_URL:-}",
      "tools": {
        "defaultStrategy": "dynamic-first",
        "routing": {
          "exit_plan_mode": "builtin-only",
          "task": "builtin-only",
          "echo": "builtin-first",
          "glob": "builtin-first",
          "grep": "builtin-first",
          "ls": "builtin-first"
        }
      }
    }
  },
  "logging": {
    "enabled": true,
    "eventsEnabled": false
  },
  "tools": []
}

